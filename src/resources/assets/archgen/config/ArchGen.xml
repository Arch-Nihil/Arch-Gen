		</BiomeSet>
		
		<BiomeSet name="Swamp">
			<Biome name="Swampland.*"/>
			<Biome name="MushroomIsland.*"/>
		</BiomeSet>
		
		<BiomeSet name="Savanna">
			<Biome name="Savanna.*"/>
		</BiomeSet>
		
		<BiomeSet name="Sand">
			<Biome name="Beach"/>
			<Biome name="Desert.*"/>
			<Biome name="Mesa.*"/>
		</BiomeSet>
		
		<BiomeSet name="Ocean">
			<Biome name="Ocean"/>
			<Biome name="FrozenOcean"/>
			<Biome name="Deep Ocean"/>
		</BiomeSet>
		
		<BiomeSet name="Continent">
			<Biome name=".*"/>
			<BiomeSet inherits='Ocean' weight='-1'/>
			<BiomeSet inherits='Beach' weight='-1'/>
		</BiomeSet>
		
		<BiomeSet name="All">
			<Biome name=".*"/>
		</BiomeSet>
	</ConfigSection>
		<!-- =========================================================================================================
		| 											  Basic Substitutions	 										 |
		========================================================================================================== -->
	<ConfigSection>
		<IfCondition condition=':= ?enableStones'>
			<Substitute name='BasicSubstitutionAll' minHeight="0" maxHeight="32">
				<OreBlock block='stone'/>
				<Replaces block='dirt'/>
				<Replaces block='sandstone'/>
				<Replaces block='gravel'/>
				<BiomeSet inherits='All'/>
			</Substitute>
			<Substitute name='BasicSubstitutionContinent' minHeight="33" maxHeight="56">
				<OreBlock block='stone'/>
				<Replaces block='dirt'/>
				<Replaces block='sandstone'/>
				<Replaces block='gravel'/>
				<BiomeSet inherits='Continent'/>
				<BiomeSet inherits='Beach'/>
			</Substitute>
			<Substitute name='BasicSubstitutionContinentSecond' minHeight="33" maxSurfRelHeight="44/64 * dimension.groundLevel">
